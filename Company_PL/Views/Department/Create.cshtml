
@model CreateDepartmentDTO

@{
    ViewData["Title"] = "Create";
}

<h1>Create Department</h1>

<br />
<br />

<form asp-action="Create">
    <div class="my-3">
        <label class="col-form-label" asp-for="Code">Code</label>
        <input class="form-control" asp-for=Code  />
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>

    <div>
        <label class="col-form-label" asp-for="Name">Name</label>
        <input class="form-control" asp-for=Name />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>

    <div>
        <label class="col-form-label" asp-for="CreateAt">CreateAt</label>
        <input class="form-control" asp-for=CreateAt />
        <span asp-validation-for="CreateAt" class="text-danger"></span>
    </div>

    <div class="my-3">
        <input type="submit" value="Create" class="btn btn-primary" />
        <a class="btn btn-light" asp-action="Index">Back To List</a>
    </div>
 
</form>

@section validationSection {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}