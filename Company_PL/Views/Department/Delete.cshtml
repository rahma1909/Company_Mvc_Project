@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Department</h1>
<br />
<br />


<form asp-action="Delete">
    <div class="my-3">
        <label class="col-form-label" asp-for="Code">Code</label>
        <input class="form-control" asp-for=Code readonly />
     
    </div>

    <div>
        <label class="col-form-label" asp-for="Name">Name</label>
        <input class="form-control" asp-for=Name readonly />
        
    </div>

    <div>
        <label class="col-form-label" asp-for="CreateAt">CreateAt</label>
        <input class="form-control" asp-for=CreateAt readonly />
      
    </div>

    <div class="my-3">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-light" asp-action="Index">Back To List</a>
    </div>

</form>



